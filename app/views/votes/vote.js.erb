<% if @post.already_voted?(current_user) == true %>
  $('.vote-'+<%= @post.id %>).css('background-color', '#F4F3BB');
  var num = parseInt($('#numb-'+<%= @post.id %>).text());
  $('.count-'+<%= @post.id %>).text(num+1);
<% else %>
 $('.vote-'+<%= @post.id %>).css('background-color', '#D1FFB6');
 var num = parseInt($('#numb-'+<%= @post.id %>).text());
  $('.count-'+<%= @post.id %>).text(num-1);
<% end %>