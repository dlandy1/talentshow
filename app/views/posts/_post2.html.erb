         <div id= "icon-<%=post.id%>" style=" width: 1011px; font-size: 20px;position: absolute; left: 10%; margin-top: 26px;">
            <%= link_to post_aprove_path(post), method: :post, data: { confirm: 'Are you sure you want to accept this post?'}, remote: :true do%>
                <%= fa_icon("check", class: "pull-left") %> 
            <%end%>
            <%= link_to post_path(post), method: :delete, data: { confirm: 'Are you sure you want to delete this post?'}, remote: :true do%>
                <%= fa_icon("close", class: "pull-right")  %> 
            <%end%>
        </div>
         <li class= "item" id= "<%=post.id%>">
                    <div style="display: table; width: 100%;">
                       <%= render partial: 'votes/vote', locals: { post: post } %>
                        <%= link_to post_path(post), method: :get, remote: :true do%>
                                <div class="open" style="padding: 7px 5px;">
                                    <div style="margin-left: 5px;" class="inlines">
                                        <p class="spacey">
                                            <span class="url" id="<%=post.url%>"><%=post.title%></span>
                                        </p>
                                        <p class="type"><%=post.kind%></p>
                                    </div>
                                    <div class="pull-right" style="margin-top: 11px;">
                                        <span class="numba"><%=post.comments.count%></span>
                                        <%= fa_icon("comment-o", class: "camnt") %>
                                        <%= image_tag(post.user.image, class: "imge shorten") %>
                                    </div>
                                </div>
                        <%end%>
                    </div>
                </li>

