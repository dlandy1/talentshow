<% @sorted_groups.each do |group,posts|   %>
 <%day=group[1].strftime("%A")%>
 <%date=group[1].strftime("%B %e")%>
  <%id=group[1].strftime("%B-%e")%>
 $('.sauce').append("<%= escape_javascript(render partial: 'date', locals: { date: date, day: day } ) %>")
  <% posts.each do |psst|%>
    $('.sauce').append("<ul class='listing' id='<%= id %>'></ul>")
    $('#<%=id%>').append("<%=escape_javascript(render partial: 'post', locals: { post: psst }) %>")
  <% end %>
<% end %>


$('.lood').html("<%= escape_javascript(render partial: 'page') %>");
